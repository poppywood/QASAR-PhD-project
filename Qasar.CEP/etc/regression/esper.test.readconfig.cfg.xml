<?xml version="1.0" encoding="UTF-8"?><esper-configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../esper-configuration-1-0.xsd">		<event-type alias="MySampleEventOne" class="com.mycompany.myapp.MySampleEventOne"/>	<event-type alias="MySampleEventTwo" class="com.mycompany.myapp.MySampleEventTwo"/>	<event-type alias="MyNoSchemaXMLEventAlias">		<xml-dom root-element-name="MyNoSchemaEvent" >			<xpath-property property-name="element1" xpath="/myevent/element1" type="number"/>		</xml-dom>	</event-type>		<event-type alias="MySchemaXMLEventAlias">		<xml-dom root-element-name="MySchemaEvent" root-element-namespace="samples:schemas:simpleSchema" schema-resource="MySchemaXMLEvent.xsd" 				default-namespace="default-name-space">			<namespace-prefix prefix="ss" namespace="samples:schemas:simpleSchema"/>			<xpath-property property-name="element1" xpath="/myevent/element1" type="number"/>		</xml-dom>	</event-type>		<event-type alias="MyMapEvent">		<map>		  	<map-property name="myInt" class="int"/>	  		<map-property name="myString" class="string"/>	  </map>	</event-type>		<event-type alias="MyLegacyTypeEvent" class="com.mycompany.package.MyLegacyTypeEvent">		<legacy-type accessor-style="public" code-generation="enabled" property-resolution-style="case_insensitive">			<method-property name="mymethodprop" accessor-method="myAccessorMethod" />			<field-property name="myfieldprop" accessor-field="myFieldName" />		</legacy-type>	</event-type>	<auto-import import-name="com.mycompany.myapp.*"/>	<auto-import import-name="com.mycompany.myapp.ClassOne"/>		<database-reference name="mydb1">    <provider-connection      provider="MySql.Data.MySqlClient"      connection-string="Server=localhost;Database=test;Uid=nesper;Pwd=password"      />		<connection-lifecycle value="pooled"/>		<lru-cache size="10"/>	</database-reference>		<database-reference name="mydb2">    <provider-connection      provider="System.Data.Odbc"      connection-string="Driver={MySQL ODBC 3.51 Driver};Server=localhost;Database=test;User=nesper;Password=password;Option=3"
      />		<connection-settings auto-commit="false" catalog="test" read-only="true" transaction-isolation="ReadCommitted" />			<connection-lifecycle value="retain"/>		<expiry-time-cache max-age-seconds="60.5" purge-interval-seconds="120.1"/>	</database-reference>	<plugin-view namespace="ext0" name="myview0" factory-class="com.mycompany.MyViewFactory0" />	<plugin-view namespace="ext1" name="myview1" factory-class="com.mycompany.MyViewFactory1" />	<plugin-aggregation-function name="func1" function-class="com.mycompany.MyMatrixAggregationMethod0" />	<plugin-aggregation-function name="func2" function-class="com.mycompany.MyMatrixAggregationMethod1" />	<plugin-pattern-guard namespace="ext0" name="guard1" factory-class="com.mycompany.MyGuardFactory0"/>	<plugin-pattern-guard namespace="ext1" name="guard2" factory-class="com.mycompany.MyGuardFactory1"/>	<plugin-pattern-observer namespace="ext0" name="observer1" factory-class="com.mycompany.MyObserverFactory0" />	<plugin-pattern-observer namespace="ext1" name="observer2" factory-class="com.mycompany.MyObserverFactory1" />	<adapter-loader name="Loader1" class-name="net.esper.support.adapter.SupportLoaderOne">		<init-arg name="name1" value="val1"/>		<init-arg name="name2" value="val2"/>	</adapter-loader>	<adapter-loader name="Loader2" class-name="net.esper.support.adapter.SupportLoaderTwo" />		<engine-settings>		<defaults>			<threading>				<listener-dispatch preserve-order="false" timeout-msec="2000"/>				<insert-into-dispatch preserve-order="false"/>				<internal-timer enabled="false" msec-resolution="1234567"/>			</threading>			<event-meta>				<class-property-resolution style="distinct_case_insensitive"/>			</event-meta>			<view-resources>				<share-views enabled="false"/>			</view-resources>			<logging>				<execution-path enabled="true"/>			</logging>		</defaults>	</engine-settings>	</esper-configuration>